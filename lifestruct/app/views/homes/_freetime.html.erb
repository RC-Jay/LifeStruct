<div id="freetimeContainer">
<%= bootstrap_form_for(:goal, url: '/goals/freetime', method: "get", layout: :inline) do |f| %>
  <h3> Free time between</h3>
  <%= f.text_field :starttime, style: "margin: 10px;", placeholder: "now", skip_label: true, inline: true, class: "small-field" %>
  <h3>and</h3>
  <%= f.text_field :endtime, style: "margin: 10px;", placeholder: "2 hours from now", skip_label: true, inline: true, class: "small-field" %>
  <%= f.submit "Free Time", class: 'btn btn-primary' %>
<% end %>
</div>